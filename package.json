{
  "name": "@flemist/ffmpeg-encode-decode",
  "version": "0.0.6",
  "description": "WASM (WebAssembly) encode/decode mp3/ogg/opus/vorbis from/to Float32Array. Well tested @ffmpeg/ffmpeg wrapper.",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14.17.6",
    "pnpm": "^7.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikolayMakhonin/ffmpeg-encode-decode.git"
  },
  "keywords": [
    "WASM",
    "WebAssembly",
    "ffmpeg",
    "encode",
    "decode",
    "encoder",
    "decoder",
    "mp3",
    "ogg",
    "opus",
    "vorbis",
    "mono",
    "stereo",
    "Float32Array"
  ],
  "author": "Nikolay Makhonin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NikolayMakhonin/ffmpeg-encode-decode/issues"
  },
  "homepage": "https://github.com/NikolayMakhonin/ffmpeg-encode-decode",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "pnpm build && pnpm run test:ci",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint:fix": "eslint --fix \"./src/**/*.ts\"",
    "build:js": "rollup -c && cpy \"**/assets/**\" \"../dist/\" --parents --cwd=src",
    "build:types": "tsc --outDir dist --declaration",
    "build": "rimraf dist && pnpm run build:js",
    "coverage-check": "nyc check-coverage --report-dir tmp/coverage/lcov --lines 50 --functions 50 --branches 50 --statements 50",
    "test": "mocha ./src/**/*.test.ts",
    "test:coverage": "rimraf tmp/coverage && nyc --all mocha ./src/**/*.test.ts && pnpm run coverage-check",
    "test:watch": "mocha --watch ./src/**/*.test.ts",
    "test:ci": "rimraf tmp/coverage && nyc --all mocha ./src/**/*.test.ts ./dist/**/*.test.cjs && pnpm run coverage-check",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "ci:test": "pnpm install --frozen-lockfile && pnpm build && pnpm run test:ci"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "3.1.9",
    "@rollup/plugin-commonjs": "=21.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.2.1",
    "@rollup/plugin-replace": "4.0.0",
    "@rollup/plugin-typescript": "8.3.2",
    "@types/assert": "1.5.6",
    "@types/expect": "24.3.0",
    "@types/fs-extra": "9.0.13",
    "@types/mocha": "9.1.1",
    "@types/node": "17.0.25",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "cpy-cli": "4.1.0",
    "eslint": "8.14.0",
    "eslint-config-pro": "2.1.1",
    "expect": "27.5.1",
    "fs-extra": "10.1.0",
    "mocha": "9.2.2",
    "music-metadata": "7.12.3",
    "nodemon": "2.0.15",
    "nyc": "15.1.0",
    "rdtsc": "2.0.0",
    "rimraf": "3.0.2",
    "rollup": "2.70.2",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-multi-input": "1.3.1",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "^4.7.4"
  },
  "optionalDependencies": {
    "@flemist/ffmpeg.wasm-core-st": "^0.10.0"
  },
  "dependencies": {
    "@flemist/async-utils": "^0.0.8",
    "@flemist/ffmpeg.wasm-st": "^0.10.1",
    "@flemist/worker-server": "^0.0.1",
    "abortcontroller-polyfill": "^1.7.3",
    "tslib": "^2.4.0"
  }
}
